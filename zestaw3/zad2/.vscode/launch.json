    "version": "0.2.0",
    "configurations": [
        {
            "name": "Tester",
            "type": "cppdbg",
            "request": "launch",
            "program": " ${workspaceFolder}/tester",
            "args": ["./tests/jeden", "1", "5", "10"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "gcc/tester"
        },
        {
            "name": "Zadanie2",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/${}",
            "args": ["./lista", "15", "in_memory"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "gcc/zadanie"
        }
    ]
}